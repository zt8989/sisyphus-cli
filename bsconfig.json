{
  "name": "sisyphus-cli",
  "version": "0.1.0",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": ["@ryyppy/rescript-promise"],
  "gentypeconfig": {
    "language": "typescript",
    "shims": {
      "Js": "Js"
    },
    "generatedFileExtension": ".gen.ts",
    "debug": {
      "all": false,
      "basic": false
    }
  }
}
